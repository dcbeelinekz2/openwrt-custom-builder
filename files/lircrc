 prog = irexec
 button = power
 config = /etc/wifiradio/sleep.sh&
end

begin
 prog = irexec
 button = +
 config = amixer -q set Speaker 8%+
end

begin
 prog = irexec
 button = -
 config = amixer -q set Speaker 8%-
end

